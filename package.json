    {
      "name": "Victor Santaella",
      "url": "https://github.com/victorst79"
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/victorst79/NaM-Bot/issues"
  },
  "homepage": "https://github.com/victorst79/NaM-Bot#readme",
  "engines": {
    "npm": ">=6.10.0",
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@types/node": "^14.0.13",
    "chalk": "^4.1.0",
    "discord-akairo": "^8.0.0",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "inversify": "^5.0.1",
    "reflect-metadata": "^0.1.13",
    "typescript": "^3.9.5"
