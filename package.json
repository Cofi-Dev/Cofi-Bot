  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/init.js",
    "dev": "nodemon dist/init.js",
    "watch": "tsc -p tsconfig.json -w",
    "format": "prettier --write \"src/**/*.*(js|ts)\"",
    "test": "mocha -r ts-node/register \"tests/**/*.spec.ts\""
  },
