{
  "name": "nam-bot",
  "version": "0.0.1",
  "description": "Multifuctional discord bot",
  "main": "init.ts",
  "scripts": {
    "start": "node dist/init.js",
    "watch": "tsc -p tsconfig.json -w",
    "dev": "nodemon dist/init.js",
    "format": "prettier --write \"src/**/*.*(js|ts)\"",
    "build": "tsc -p tsconfig.json",
    "test": "mocha -r ts-node/register tests/**/*.spec.ts",
    "test-api": "mocha -r ts-node/register tests/API/*.spec.ts",
    "test-services": "mocha -r ts-node/register tests/Services/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victorst79/NaM-Bot.git"
  },
  "author": "Victor Santaella",
  "maintainers": [
    {
      "name": "Victor Santaella",
      "url": "https://github.com/victorst79"
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/victorst79/NaM-Bot/issues"
  },
  "homepage": "https://github.com/victorst79/NaM-Bot#readme",
  "engines": {
    "npm": ">=6.10.0",
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@discordjs/opus": "^0.3.3",
    "@types/node": "^14.0.13",
    "chalk": "^4.1.0",
    "cors": "^2.8.5",
    "discord-akairo": "^8.0.0",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "ffmpeg": "0.0.4",
    "ffmpeg-static": "^4.2.5",
    "fluent-ffmpeg": "^2.1.2",
    "inversify": "^5.0.1",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "moment-duration-format": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "typescript": "^3.9.5",
    "ytdl-core": "^4.2.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.11",
    "@types/chai-http": "^4.2.0",
    "@types/express": "^4.17.9",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^7.1.2",
    "nodemon": "^2.0.4",
    "prettier": "2.0.5",
    "ts-mockito": "^2.6.1",
    "ts-node": "^8.10.2"
  }
}
